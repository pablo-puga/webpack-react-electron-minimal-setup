{
  "name": "webpack-react-electron-minimal-setup",
  "version": "0.3.4",
  "description": "A minimal setup for an Electron APP using React and Webpack",
  "main": "app/main.js",
  "scripts": {
    "bundle-prod": "webpack --config webpack.config.prod.js",
    "prod-app": "cross-env ENV=prod electron ./app/main.js",
    "dev-app": "cross-env ENV=dev electron ./app/main.dev.js",
    "start-server": "cross-env ENV=dev webpack-dev-server --config webpack.config.dev.js",
    "dev": "cross-ENV START_APP=yes npm run start-server",
    "prod": "npm run bundle-prod && npm run prod-app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pablo-puga/webpack-react-electron-minimal-setup.git"
  },
  "keywords": [
    "Electron",
    "React",
    "Webpack",
    "EcmaScript 6",
    "Babel",
    "Gulp"
  ],
  "author": "Pablo Puga <pablo-puga>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pablo-puga/webpack-react-electron-minimal-setup/issues"
  },
  "homepage": "https://github.com/pablo-puga/webpack-react-electron-minimal-setup#readme",
  "devDependencies": {
    "concurrently": "^3.4.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.1",
    "devtron": "^1.4.0",
    "electron": "^1.6.6",
    "extract-text-webpack-plugin": "^2.1.0",
    "style-loader": "^0.17.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {}
}
